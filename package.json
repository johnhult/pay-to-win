	"scripts": {
		"build": "onerror 'webpack --config webpack.prod.js --mode production --display-error-details' -t 'The build has failed' -m 'Check the console to find out what went wrong.' -s 'Submarine'",
		"build-storybook": "build-storybook",
		"build:storybook": "build-storybook",
		"commands": "npx ntl",
		"copy:assets": "cp -R src/assets dist/ && cp src/* dist/",
		"copy:meta": "cp src/robots.txt dist/ && cp src/favicon.ico dist/ && cp src/sitemap.xml dist/ && cp src/crossdomain.xml dist/ && cp src/_headers dist/ && cp src/_redirects dist/ && cp src/manifest.webmanifest dist/",
		"dev": "webpack-dev-server --config webpack.dev.js --open --mode development",
		"figmagic": "yarn figmagic:clean && yarn figmagic:saveFromApi && yarn figmagic:build && yarn figmagic:images",
		"figmagic:build": "node --experimental-modules $npm_package_config_figmagicPath/bin/index.mjs",
		"figmagic:clean": "rm -rf specs/ && rm -rf tokens/ && rm -rf grid/ && rm -rf figma && mkdir tokens && cp $npm_package_config_figmagicPath/.gridTemplate.mjs tokens/ && mv tokens/.gridTemplate.mjs tokens/grid.mjs",
		"figmagic:downloadGraphics": "node --experimental-modules $npm_package_config_figmagicPath/bin/downloadImages.mjs --graphics",
		"figmagic:downloadImages": "node --experimental-modules $npm_package_config_figmagicPath/bin/downloadImages.mjs",
		"figmagic:getGraphics": "node --experimental-modules $npm_package_config_figmagicPath/bin/getImages.mjs --graphics",
		"figmagic:getImages": "node --experimental-modules $npm_package_config_figmagicPath/bin/getImages.mjs",
		"figmagic:graphics": "yarn figmagic:getGraphics && yarn figmagic:downloadGraphics && yarn figmagic:optimizeGraphics",
		"figmagic:images": "yarn figmagic:getImages && yarn figmagic:downloadImages",
		"figmagic:optimizeGraphics": "svgo -f ./specs/graphics/ --multipass --disable=removeViewBox --enable=removeDimensions",
		"figmagic:saveFromApi": "mkdir -p figma && wget 'https://api.figma.com/v1/files/KLLDK9CBSg7eAayiTY3kVqC8' --header='X-Figma-Token: 1405-0ca6a5be-8e27-40ee-8c40-778d39ab2316' -O figma/figma.json",
		"figmagic:tokens": "yarn figmagic:clean && yarn figmagic:saveFromApi && yarn figmagic:build",
		"install:wget": "brew install wget --with-libressl",
		"optimize:graphics": "svgo -f ./dist/assets/**/ --multipass --disable=removeViewBox --enable=removeDimensions",
		"plop": "npx plop",
		"postbuild": "yarn copy:meta && yarn test:size-limit && react-snap",
		"precommit": "echo Running pre-commit testing and linting... && yarn build && pretty-quick --staged && npm --no-git-tag-version version patch && git add package.json",
		"setup": "yarn && echo '✔' Used Yarn to install all dependencies && npx storybook init -f && yarn setup:welcome",
		"setup:welcome": "echo '✔' Set up React Storybook && echo '🎉' New commands are available! '🎉' && echo '—' yarn dev: Start development server && echo '—' yarn build: Output a static build && echo '—' yarn plop function/class/model: Create function/class/model with test for function/class && echo '—' yarn plop component: Create React component, with Styled Components file and Storybook story && echo '—' yarn plop view: Create basic React view && echo '—' yarn plop componentclass: Create React classed component, with Styled Components file and Storybook story && echo '—' yarn storybook: Start React Storybook && echo '—' yarn build:storybook: Build React Storybook && echo '—' yarn commands: See a list of all available commands",
		"start": "test && yarn run dev",
		"storybook": "start-storybook -p 6006",
		"test": "yarn test:jest",
		"test:accessibility": "npx axe $npm_package_config_siteUrl --save axe.report.json && npx pa11y --reporter csv $npm_package_config_siteUrl > pa11y.report.csv",
		"test:performance": "npx lighthouse $npm_package_config_siteUrl --view",
		"test:all": "yarn test:e2e && yarn test:visual",
		"test:jest": "jest",
		"test:e2e": "jest __tests__/e2e",
		"test:e2e:debug": "NODE_ENV=debug yarn test:e2e",
		"test:visual": "jest __tests__/visual",
		"test:size-limit": "size-limit"
	},
	"size-limit": [
		{
			"limit": "120 KB",
			"path": "dist/*.js",
			"webpack": false
		}
	],
	"reactSnap": {
		"source": "dist"
	}
}
